{
	"name": "fetchMasterData",
	"properties": {
		"description": "it will bring the master Data from the one profile",
		"activities": [
			{
				"name": "fetchDataFromOneProfile",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
