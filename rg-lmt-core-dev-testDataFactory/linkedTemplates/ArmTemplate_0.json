{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "rg-lmt-core-dev-testDataFactory"
		},
		"AzureSqlDatabase1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabase1'"
		},
		"AzureSqlDatabase1_servicePrincipalKey": {
			"type": "secureString",
			"metadata": "Secure string for 'servicePrincipalKey' of 'AzureSqlDatabase1'"
		},
		"AzureTableStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureTableStorage1'"
		},
		"AzureSqlDatabase1_properties_typeProperties_servicePrincipalId": {
			"type": "string",
			"defaultValue": "5eb170f9-f828-4121-936c-288eb43b050e"
		},
		"AzureSqlDatabase1_properties_typeProperties_tenant": {
			"type": "string",
			"defaultValue": "72f988bf-86f1-41af-91ab-2d7cd011db47"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/AzureSqlDatabase1')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureSqlDatabase",
				"typeProperties": {
					"connectionString": "[parameters('AzureSqlDatabase1_connectionString')]",
					"servicePrincipalId": "[parameters('AzureSqlDatabase1_properties_typeProperties_servicePrincipalId')]",
					"servicePrincipalKey": {
						"type": "SecureString",
						"value": "[parameters('AzureSqlDatabase1_servicePrincipalKey')]"
					},
					"tenant": "[parameters('AzureSqlDatabase1_properties_typeProperties_tenant')]"
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/AzureTableStorage1')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureTableStorage",
				"typeProperties": {
					"connectionString": "[parameters('AzureTableStorage1_connectionString')]"
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/AttributeValueLaborEntries')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "AzureTableStorage1",
					"type": "LinkedServiceReference"
				},
				"annotations": [],
				"type": "AzureTable",
				"schema": [],
				"typeProperties": {
					"tableName": "AttributeValueLaborEntries"
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/AzureTableStorage1')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/laborCategoriesByPartner_Sink')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "AzureTableStorage1",
					"type": "LinkedServiceReference"
				},
				"annotations": [],
				"type": "AzureTable",
				"schema": [],
				"typeProperties": {
					"tableName": "laborCategoriesByPartner"
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/AzureTableStorage1')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/dataMall_Taxi01Attr_Value')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "AzureSqlDatabase1",
					"type": "LinkedServiceReference"
				},
				"annotations": [],
				"type": "AzureSqlTable",
				"schema": [
					{
						"name": "AbbreviatedValue",
						"type": "varchar"
					},
					{
						"name": "AttributeValueDDObjectID",
						"type": "bigint",
						"precision": 19
					},
					{
						"name": "AttributeValueEffectiveDate",
						"type": "datetime",
						"precision": 23,
						"scale": 3
					},
					{
						"name": "AttributeValueName",
						"type": "nvarchar"
					},
					{
						"name": "BusinessEffectiveEndDate",
						"type": "datetime",
						"precision": 23,
						"scale": 3
					},
					{
						"name": "BusinessEffectiveStartDate",
						"type": "datetime",
						"precision": 23,
						"scale": 3
					},
					{
						"name": "ChangeRequestCode",
						"type": "nvarchar"
					},
					{
						"name": "CommentText",
						"type": "nvarchar"
					},
					{
						"name": "CreatedBy",
						"type": "varchar"
					},
					{
						"name": "CreatedDate",
						"type": "datetime",
						"precision": 23,
						"scale": 3
					},
					{
						"name": "DQRPendingReviewIndicator",
						"type": "bit"
					},
					{
						"name": "NumericValue",
						"type": "decimal",
						"precision": 16,
						"scale": 6
					},
					{
						"name": "OldDomainValueID",
						"type": "int",
						"precision": 10
					},
					{
						"name": "OwningAttributeDDObjectID",
						"type": "bigint",
						"precision": 19
					},
					{
						"name": "SRActionID",
						"type": "int",
						"precision": 10
					},
					{
						"name": "StatusID",
						"type": "int",
						"precision": 10
					},
					{
						"name": "UpdatedBy",
						"type": "varchar"
					},
					{
						"name": "UpdatedDate",
						"type": "datetime",
						"precision": 23,
						"scale": 3
					},
					{
						"name": "WorkingActionID",
						"type": "int",
						"precision": 10
					}
				],
				"typeProperties": {}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/AzureSqlDatabase1')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/fetchMasterData')]",
			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"description": "it will bring the master Data from the one profile",
				"activities": [
					{
						"name": "fetchDataFromOneProfile",
						"type": "Copy",
						"dependsOn": [],
						"policy": {
							"timeout": "7.00:00:00",
							"retry": 0,
							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
						},
						"userProperties": [
							{
								"name": "Source",
								"value": "dbo.TAXY01AttributeValue"
							},
							{
								"name": "Destination",
								"value": "AttributeValueLaborEntries"
							}
						],
						"typeProperties": {
							"source": {
								"type": "AzureSqlSource",
								"sqlReaderQuery": "SELECT \ta.AttributeValueDDObjectID as ID,\n\t\tltrim(rtrim(a.AttributeValueName)) as [NAME] ,\n\t\tltrim(rtrim(c.DataDictionaryObjectShortDesc)) as [Description] ,\n\t\tltrim(rtrim(g.AttributeValueName)) as [UtilizationEligibleIndicator] ,\n\t\tltrim(rtrim(e.AttributeValueName)) as [CustomerFacingIndicator] ,\n\t\tltrim(rtrim(d.AttributeValueName)) as [BillableIndicator] ,\n\t\tltrim(rtrim(f.AttributeValueName)) as [CategoryGroup] ,\n\t\ta.BusinessEffectiveStartDate [EffectiveStartDate],\n\t\ta.BusinessEffectiveEndDate [EffectiveEndDate]\nFROM    dbo.TAXY01AttributeValue a \n\t\tJOIN dbo.TAXY01Attribute b ON b.AttributeDataDictionaryObjID = a.OwningAttributeDDObjectID\n\t\tJOIN dbo.TAXY01DataDictionaryObject c ON c.DataDictionaryObjectID = a.AttributeValueDDObjectID \n\t\t\n\t\tLEFT JOIN ( SELECT  a.AttributeValueName ,\n\t\t\t\t\t\t\tc.ChildAttributeValueDDObjID\n\t\t\t\t\tFROM    dbo.TAXY01AttributeValue a\n\t\t\t\t\t\t\tJOIN dbo.TAXY01Attribute b ON b.AttributeDataDictionaryObjID = a.OwningAttributeDDObjectID\n\t\t\t\t\t\t\tJOIN dbo.TAXY01AttributeValueRltn c ON c.ParentAttributeValueDDObjID = a.AttributeValueDDObjectID\n\t\t\t\t\tWHERE   b.AttributeName = 'Labor Category Billable Indicator'\n\t\t\t\t\t) d ON d.ChildAttributeValueDDObjID = a.AttributeValueDDObjectID \n\t\t\n\t\tLEFT JOIN ( SELECT  a.AttributeValueName ,\n\t\t\t\t\t\t\tc.ChildAttributeValueDDObjID\n\t\t\t\t\tFROM    dbo.TAXY01AttributeValue a\n\t\t\t\t\t\t\tJOIN dbo.TAXY01Attribute b ON b.AttributeDataDictionaryObjID = a.OwningAttributeDDObjectID\n\t\t\t\t\t\t\tJOIN dbo.TAXY01AttributeValueRltn c ON c.ParentAttributeValueDDObjID = a.AttributeValueDDObjectID\n\t\t\t\t\tWHERE   b.AttributeName = 'Labor Category Task Related Indicator'\n\t\t\t\t\t) e ON e.ChildAttributeValueDDObjID = a.AttributeValueDDObjectID \n\t\tLEFT JOIN ( SELECT  a.AttributeValueName ,\n\t\t\t\t\t\t\tc.ChildAttributeValueDDObjID\n\t\t\t\t\tFROM    dbo.TAXY01AttributeValue a\n\t\t\t\t\t\t\tJOIN dbo.TAXY01Attribute b ON b.AttributeDataDictionaryObjID = a.OwningAttributeDDObjectID\n\t\t\t\t\t\t\tJOIN dbo.TAXY01AttributeValueRltn c ON c.ParentAttributeValueDDObjID = a.AttributeValueDDObjectID\n\t\t\t\t\tWHERE   b.AttributeName = 'Labor Category Group Name'\n\t\t\t\t\t) f ON f.ChildAttributeValueDDObjID = a.AttributeValueDDObjectID \n\t\t\n\t\tLEFT JOIN ( SELECT  a.AttributeValueName ,\n\t\t\t\t\t\t\tc.ChildAttributeValueDDObjID\n\t\t\t\t\tFROM    dbo.TAXY01AttributeValue a\n\t\t\t\t\t\t\tJOIN dbo.TAXY01Attribute b ON b.AttributeDataDictionaryObjID = a.OwningAttributeDDObjectID\n\t\t\t\t\t\t\tJOIN dbo.TAXY01AttributeValueRltn c ON c.ParentAttributeValueDDObjID = a.AttributeValueDDObjectID\n\t\t\t\t\tWHERE   b.AttributeName = 'Labor Category Utilization Eligible Indicator'\n\t\t\t\t\t) g ON g.ChildAttributeValueDDObjID = a.AttributeValueDDObjectID \n\t\t\nWHERE   AttributeName = 'Labor Category Name'\n",
								"queryTimeout": "02:00:00"
							},
							"sink": {
								"type": "AzureTableSink",
								"azureTableInsertType": "replace",
								"azureTableDefaultPartitionKeyValue": "LaborCategoriesByPartner",
								"writeBatchSize": 10000
							},
							"enableStaging": false
						},
						"inputs": [
							{
								"referenceName": "dataMall_Taxi01Attr_Value",
								"type": "DatasetReference",
								"parameters": {}
							}
						],
						"outputs": [
							{
								"referenceName": "laborCategoriesByPartner_Sink",
								"type": "DatasetReference",
								"parameters": {}
							}
						]
					}
				],
				"annotations": []
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/datasets/dataMall_Taxi01Attr_Value')]",
				"[concat(variables('factoryId'), '/datasets/laborCategoriesByPartner_Sink')]"
			]
		}
	]
}